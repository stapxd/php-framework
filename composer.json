{
    "autoload": {
        "psr-4": {
            "Vendor\\Foundation\\": "vendor/Foundation/",
            "Vendor\\App\\": "vendor/App/",
            "Vendor\\App\\Http\\": "vendor/App/Http/",
            "Vendor\\Database\\": "vendor/Database/",
            "Vendor\\Database\\Schema\\": "vendor/Database/Schema/",
            "Vendor\\Database\\Builders\\": "vendor/Database/Builders/",
            "Vendor\\Auth\\": "vendor/Auth/",
            "Vendor\\Facades\\": "vendor/Facades/",
            "Vendor\\General\\": "vendor/General/",
            "Migration\\": "migrations/",
